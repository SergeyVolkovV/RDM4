					</valuesProvider>
				</attribute>
				<attribute defaultValue="Use App Connection Credentials" name="useUrlResourcesForAuthentication" label="Global Authentication Strategy" type="enum" required="true" enumValues="Use App Connection Credentials,Pass Credentials Manually">
					<description>Determines whether the RDM steps in synchronization and load plans use the credentials from the configured RDM web application connection (App Connection Name) or manually passed credentials from input.</description>
				</attribute>
				<attribute name="dbType" label="Database type" type="enum" required="true" enumValues="Apache Derby,MS SQL,Oracle,PostgreSQL">
