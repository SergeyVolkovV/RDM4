				<attribute visible="false" defaultValue="gpk" name="generatedPrimaryKeyName" label="Generated PK name" type="string" required="true"/>
				<attribute defaultValue="20" name="maxPageSize" label="Maximum records per page" type="integer" required="true">
					<description>Specifies the maximum number of records shown on one page in the RDM web application as a default setting for all users.</description>
				</attribute>
				<attribute defaultValue="English" name="language" label="Application GUI language" type="enum" required="true" enumValues="Czech,English,German,Russian">
					<description>Specifies the language for properties used in the RDM web application as a default setting for all users.</description>
				</attribute>
				<attribute defaultValue="English" name="docLanguage" label="Generated documentation language" type="enum" required="true" enumValues="Czech,English">
					<description>Specifies the language for generated documentation.</description>
				</attribute>
				<attribute name="returnEmailAddress" label="Return email address" type="string" required="false">
					<description>The email address displayed in the From line of received notification email messages.</description>
				</attribute>
				<attribute defaultValue="false" name="auditing" label="Auditing" type="boolean" required="true">
					<description>Specifies whether the RDM web application provides audit logs for defined operations. If set to true, the logging.xml file will be created upon generating web application configuration. </description>
				</attribute>
				<attribute defaultValue="1900-01-01 00:00:00" name="fromInfinity" label="Infinity from" type="string">
					<description>Minimum possible value for a FROM business date.</description>
				</attribute>
				<attribute defaultValue="2099-12-31 00:00:00" name="toInfinity" label="Infinity to" type="string">
