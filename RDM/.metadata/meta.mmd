								<node displayTemplate="{@parentColumn}-&gt;{@childColumn}" additions="ACCEPT" deletions="ACCEPT" containerPath="representativeForeignKey" matchTemplate="{@parentColumn}-{@childColumn}" attributeFilter="parentColumn,childColumn">
				        </node>
							</subNodes>
						</node>
						<node displayTemplate="Domain {@name}" deletions="IGNORE" containerPath="domains" matchTemplate="{@name}" attributeFilter="name,size,type">
		</node>
					</structure>
				</editor>
